{
  "name": "@project/identity-service",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "local": "dotenv -e .env -- ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
    "prisma:generate": "dotenv -e .env -- prisma generate",
    "prisma:seed": "dotenv -e .env -- prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-express": "^11.1.12",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "7.4.0",
    "@project/core": "*",
    "aws-sdk": "^2.1693.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^25.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^11.0.0",
    "prisma": "7.4.0",
    "ts-node-dev": "^2.0.0"
  }
}
