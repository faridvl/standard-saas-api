    "@nestjs/platform-express": "^11.1.12",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "7.4.0",
    "@project/core": "*",
    "dotenv": "^17.2.3",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/pg": "^8.16.0",
    "prisma": "7.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.0"
  }
